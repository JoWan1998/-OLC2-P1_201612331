S -> ESTRUCTURAMAIN

ESTRUCTURAMAIN -> MAIN DOS PUNTOS PRECUERPO

PRECUERPO -> PRECUERPO CUERPO
            |CUERPO

CUERPO -> VARIABLE_TEMPORALES
         |LABEL
         |GOTO_LABEL
         |EXPRESION
         |EXPRESION_ARTIMETICA
         |CONVERSIONES
         |INSTRUCCIONES_BIT
         |INSTRUCCIONES_LOGICAS
         |INSTRUCCIONES_RELACIONES
         |ARREGLOS
         |ESTRUCTURA_IF

INSTRUCCIONES_LOGICAS -> INSTRUCCIONES_LOGICAS LOGICAS 
                        |LOGICAS

LOGICAS -> DIFERENCIA LOGICAS puntoycoma
          |LOGICAS DOBLEAMPERSON LOGICAS puntoycoma
          |LOGICAS DOBLEBARRA LOGICAS puntoycoma
          |LOGICAS XOR LOGICAS puntoycoma
          |VARIABLE_TEMPORALES  

ESTRUCTURA_IF -> IF PAR_A EXPRESION_IF PAR_C GOTO IDENTIFICADOR puntoycoma 

EXPRESION_IF -> INSTRUCCIONES_BIT
               |INSTRUCCIONES_LOGICAS
               |INSTRUCCIONES_RELACIONES

ARREGLOS -> VARIABLE_TEMPORALES IGUAL ARRAY PAR_A PAR_C puntoycoma
           |VARIABLE_TEMPORALES CORCHETE_A EXPRESION_ARREGLO CORCHETE_C IGUAL NUMERO puntoycoma

EXPRESION_ARREGLO -> NUMERO
                    |CADENA   
                    |VARIABLE_TEMPORALES     

INSTRUCCIONES_RELACIONES -> INSTRUCCIONES_RELACIONES RELACIONALES
                           |RELACIONALES

RELACIONALES ->  RELACIONALES DOBLEIGUAL RELACIONALES
                |RELACIONALES DIFERENCIA RELACIONALES
                |RELACIONALES MAYORIGUAL RELACIONALES  
                |RELACIONALES MENORIGUAL RELACIONALES
                |RELACIONALES MAYOR RELACIONALES
                |RELACIONALES MENOR RELACIONALES
                
INSTRUCCIONES_BIT -> INSTRUCCIONES_BIT BIT
                    |BIT

BIT -> NOT BIT 
      |BIT AMPERSO BIT 
      |BIT BARRA BIT
      |BIT ELEVACION BIT
      |BIT DOBLEMENOR BIT
      |BIT DOBLEMAYOR BIT
      |VARIABLE_TEMPORALES

EXPRESION_ARTIMETICA -> EXPRESION_ARTIMETICA ARITMETICA
                       |ARITMETICA

ARITMETICA -> ARITMETICA MAS ARITMETICA
             |ARITMETICA MENOS ARITMETICA
             |ARITMETICA MULTIPLICACION ARITMETICA
             |ARITMETICA DIVISION ARITMETICA
             |ARITMETICA RESIDUO ARITMETICA
             |ARITMETICA ABSOLUTO ARITMETICA
             |VARIABLE_TEMPORALES

CONVERSIONES -> VARIABLE_TEMPORALES PAR_A ASIGNACION PAR_C VARIABLE_TEMPORALES puntoycoma

ASIGNACION -> int
             |float
             |char

EXPRESION -> EXPRESION CUERPO_EXPRESION
            |CUERPO_EXPRESION

CUERPO_EXPRESION -> ASIGNACION_NUMERICA
                   |ASIGNACION_CADENA
                   |COPIA_SIMPLE
                   |NEGATIVO
                   |PUNTERO
                   |DESTRUYEVARIABLE
                   |IMPRIME
                   |LEERENTRADA
                   |EXIT

EXIT -> exit puntoycoma 

LEERENTRADA -> VARIABLE_TEMPORALES IGUAL LEER

LEER -> READ PAR_A PAR_C puntoycoma

IMPRIME -> PRINT PAR_A VARIABLE_TEMPORALES PAR_C puntoycoma 
          |PRINT PAR_A ARREGLOS PAR_C puntoycoma

DESTRUYEVARIABLE -> UNSET PAR_A VARIABLE_TEMPORALES PAR_C puntoycoma                   
 
PUNTERO -> VARIABLE_TEMPORALES IGUAL AMPERSON VARIABLE_TEMPORALES puntoycoma

NEGATIVO -> VARIABLE_TEMPORALES IGUAL MENOS VARIABLE_TEMPORALES puntoycoma

COPIA_SIMPLE -> VARIABLE_TEMPORALES IGUAL VARIABLE_TEMPORALES puntoycoma

ASIGNACION_CADENA -> VARIABLE_TEMPORALES IGUAL EXPRESIONCADENA puntoycoma                   

ASIGNACION_NUMERICA -> VARIABLE_TEMPORALES IGUAL  NUMERO puntoycoma

GOTO_LABEL -> goto IDENTIFICADOR puntoycoma

LABEL -> IDENTIFICADOR DOSPUNTOS         

VARIABLE_TEMPORALES -> Dollar IDENTIFICADOR numero



